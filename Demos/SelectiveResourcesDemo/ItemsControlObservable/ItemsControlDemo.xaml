<UserControl x:Class="SelectiveResourcesDemo.ItemsControlObservable.ItemsControlDemo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SelectiveResourcesDemo.ItemsControlObservable"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel>
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
            <Button Content="Go" Margin="4" Padding="4" Click="Button_Click"/>
        </StackPanel>
        <ListBox x:Name="lb"/>
    </DockPanel>
</UserControl>
